# Test environment variables
ENV=test
DEBUG=True
TESTING=True

# Database - Using Supabase PostgreSQL
# Replace with your actual Supabase database credentials
POSTGRES_SERVER=db.your-project-ref.supabase.co
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your-database-password
POSTGRES_DB=postgres
SQLALCHEMY_DATABASE_URI=postgresql+psycopg2://postgres:your-database-password@db.your-project-ref.supabase.co:5432/postgres

# Supabase
SUPABASE_URL=https://test-supabase.co
SUPABASE_KEY=test-supabase-key
SUPABASE_JWT_SECRET=test-jwt-secret

# Security
SECRET_KEY=test-secret-key
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=60
